{
  "expo": {
    "scheme": "SolveIt",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./src/assets/icon-transparent.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.planob.solveit"
    },
    "ios": {
      "bundleIdentifier": "com.planob.solveit",
      "supportsTablet": true
    },
    "name": "SolveIt",
    "slug": "SolveIt",
    "icon": "./src/assets/adaptive-icon.png"
  }
}
